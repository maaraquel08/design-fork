{
  "name": "uifork-vue",
  "version": "0.0.1",
  "description": "CLI tool and Vue 3 components for managing UI component versions and switching between them",
  "keywords": [
    "ab-testing",
    "cli",
    "components",
    "vue",
    "switcher",
    "ui",
    "versioning"
  ],
  "license": "MIT",
  "bin": {
    "uifork-vue": "./cli.js"
  },
  "files": [
    "dist",
    "cli.js",
    "lib",
    "version-sync.js"
  ],
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "vite build",
    "build:lib": "vite build",
    "dev": "vite build --watch",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.6.0",
    "chokidar": "^3.5.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.0",
    "typescript": "^5.3.0",
    "vite": "^7.3.1",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vite-plugin-dts": "^4.5.4",
    "vue": "^3.5.0"
  },
  "peerDependencies": {
    "vue": ">=3.3.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
