{
  "name": "uifork-vue",
  "version": "0.0.4-beta.1",
  "private": true,
  "description": "A Vue 3 fork of uifork - dev tool for exploring UI ideas directly inside your Vue app",
  "keywords": [
    "vue",
    "vue3",
    "cli",
    "components",
    "ui",
    "versioning",
    "prototyping",
    "ab-testing"
  ],
  "license": "MIT",
  "files": [
    "packages/uifork-vue/dist",
    "packages/uifork-vue/cli.js",
    "packages/uifork-vue/lib",
    "packages/uifork-vue/src",
    "packages/uifork-vue/package.json",
    "packages/uifork-vue/README.md",
    "packages/uifork-vue/vite.config.ts",
    "packages/uifork-vue/tsconfig.json"
  ],
  "main": "./packages/uifork-vue/dist/index.js",
  "module": "./packages/uifork-vue/dist/index.mjs",
  "types": "./packages/uifork-vue/dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./packages/uifork-vue/dist/index.d.ts",
      "import": "./packages/uifork-vue/dist/index.mjs",
      "require": "./packages/uifork-vue/dist/index.js"
    }
  },
  "bin": {
    "uifork-vue": "./packages/uifork-vue/cli.js"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "prepare": "cd packages/uifork-vue && npm run build",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:all": "turbo run dev",
    "sandbox": "turbo run dev --filter=sandbox",
    "sandbox:full": "turbo run dev --filter=uifork --filter=sandbox",
    "sandbox:watch": "cd apps/sandbox && npm run watch",
    "marketing": "turbo run dev --filter=marketing",
    "marketing:full": "turbo run dev --filter=uifork --filter=marketing",
    "marketing:watch": "cd apps/marketing && npm run watch",
    "dev:uifork": "turbo run dev --filter=uifork",
    "dev:uifork-vue": "turbo run dev --filter=uifork-vue",
    "sandbox-vue": "turbo run dev --filter=sandbox-vue",
    "sandbox-vue:full": "turbo run dev --filter=uifork-vue --filter=sandbox-vue",
    "sandbox-vue:watch": "cd apps/sandbox-vue && npm run watch",
    "test": "turbo run test",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "fmt": "oxfmt",
    "fmt:check": "oxfmt --check"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.6.0",
    "chokidar": "^3.5.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "ws": "^8.14.2"
  },
  "peerDependencies": {
    "vue": ">=3.3.0"
  },
  "devDependencies": {
    "oxfmt": "^0.27.0",
    "oxlint": "^1.42.0",
    "turbo": "^2.8.0"
  },
  "packageManager": "npm@10.9.0"
}
